<%- if controller_name != 'sessions' %>
  <%= link_to "ログイン", new_session_path(resource_name), class: "btn rounded-pill" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "新規メンバー登録", new_registration_path(resource_name), class: "btn rounded-pill" %><br />
<% end %>
